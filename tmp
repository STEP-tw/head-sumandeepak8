const isOnlyCount = function (inputArg) {
    return inputArg.match(/^-/) != null && isFinite(inputArg);
};

const isOptionWithCount = function (inputArg) {
    return inputArg.match(/^-/) != null && isNaN(inputArg[1]) && inputArg.length > 2;
};

const isOnlyOption = function (inputArg) {
    return inputArg.match(/^-/) != null && isNaN(inputArg) && inputArg.length == 2;
};

const objectFunction = function (inputArg) {
    const OptionAndCountObject = {
        isOnlyCount: {
            result: isOnlyCount(inputArg[0]),
            true: inputArg[0]
        },

        isOnlyOption: {
            result: isOnlyOption(inputArg[0]),
            true: inputArg
        },

        isOptionWithCount: {
            result: isOptionWithCount(inputArg[0]),
            true: inputArg[0]
        }
    }
   data = Object.values(OptionAndCountObject).filter((x)=>{
         if(x['result'] == true)
         return x['true']
    });
    
    return data[0]['true'];
}
console.log(objectFunction(['-a','4']));

